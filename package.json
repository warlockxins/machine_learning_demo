{
    "name": "machine_learning_demo",
    "sideEffects": false,
    "version": "0.1.1",
    "description": "parse and use csv files, then use machine learning to predict data. Display resulting net values in a graph",
    "main": "index.js",
    "scripts": {
        "serve": "NODE_ENV=dev vue-cli-service serve",
        "build": "NODE_ENV=production vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/warlockxins/machine_learning_demo.git"
    },
    "keywords": [
        "machine",
        "learning",
        "csv",
        "parsing",
        "AI"
    ],
    "author": "Aleksandrs Semjonovs",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/warlockxins/machine_learning_demo/issues"
    },
    "homepage": "https://github.com/warlockxins/machine_learning_demo#readme",
    "dependencies": {
        "bulma": "^0.7.4",
        "machine-learning": "git+https://bitbucket.org/asemionov/machine_learning.git",
        "papaparse": "^4.5.0",
        "vue": "^2.5.21",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.2.0",
        "@vue/cli-plugin-eslint": "^3.2.0",
        "@vue/cli-service": "^3.2.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0-0",
        "vue-template-compiler": "^2.5.17"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
